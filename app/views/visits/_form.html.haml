= simple_form_for @visit, :html => { :class => 'form-horizontal' } do |f|
  = f.association :user
  = f.input :ip
  = f.input :ua
  = f.input :created
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), visits_path, :class => 'btn'
